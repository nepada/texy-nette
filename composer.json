{
    "name": "nepada/texy-nette",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Texy integration for Nette.",
    "keywords": ["nette", "latte", "texy"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/template-factory/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "nette/utils": "^3.1.3@dev",
        "texy/texy": "^3.1.3@dev"
    },
    "require-dev": {
        "nette/tester": "2.4.0",
        "phpstan/phpstan": "0.12.90",
        "phpstan/phpstan-strict-rules": "0.12.9",
        "pepakriz/phpstan-exception-rules": "0.11.7",
        "phpstan/phpstan-nette": "0.12.21",
        "nepada/phpstan-nette-tester": "0.3.1",
        "spaze/phpstan-disallowed-calls": "1.4.0",
        "php-parallel-lint/php-parallel-lint": "1.3.0",
        "nepada/coding-standard": "7.4.0",
        "nette/bootstrap": "^3.0@dev",
        "nette/di": "^3.0.5@dev",
        "latte/latte": "^2.7@dev",
        "nepada/template-factory": "2.2.0@dev",
        "nette/http": "^3.0@dev"
    },
    "suggest": {
        "nette/di": "for integration with Nette DI container",
        "latte/latte": "to use Latte with custom Texy! macros in templates",
        "nepada/template-factory": "to automatically inject required services into Latte templates"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\Bridges\\": "src/Bridges/",
            "Nepada\\Texy\\": "src/Texy/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    }
}
