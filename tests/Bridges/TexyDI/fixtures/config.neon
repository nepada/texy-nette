services:
    texyFactory:
        implement: Nepada\Texy\ITexyFactory
        factory: @Nepada\Texy\TexyFactory::create
        setup:
            - $obfuscateEmail(false)
            - $allowedTags(false)

extensions:
    templateFactory: Nepada\Bridges\TemplateFactoryDI\TemplateFactoryExtension
    texy: Nepada\Bridges\TexyDI\TexyExtension

texy:
    factories:
        custom: @texyFactory

application:
    scanDirs: false

di:
    debugger: false
